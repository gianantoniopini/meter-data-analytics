{
  "name": "meter-data-analytics-back-end",
  "version": "1.0.0",
  "private": true,
  "description": "Node.js Restful API with Node.js, Express and MongoDB",
  "main": "src/server.ts",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "nodemon --exec ts-node --require tsconfig-paths/register src/server.ts",
    "start:production": "ts-node --require tsconfig-paths/register build/back-end/src/server.js",
    "test": "jest --runInBand --verbose"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "gianantoniopini",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.2.0",
    "got": "^11.8.2",
    "http-status-codes": "^2.1.4",
    "mongoose": "^6.0.5",
    "tough-cookie": "^4.0.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.9.1",
    "@types/supertest": "^2.0.11",
    "@types/tough-cookie": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jest": "^27.4.3",
    "mongodb-memory-server": "^8.0.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
