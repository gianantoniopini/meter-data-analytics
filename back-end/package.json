{
  "name": "meter-data-analytics-back-end",
  "version": "1.0.0",
  "private": true,
  "description": "Node.js Restful API with Node.js, Express and MongoDB",
  "main": "src/server.ts",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "nodemon --exec ts-node --require tsconfig-paths/register src/server.ts",
    "start:production": "ts-node --require tsconfig-paths/register build/back-end/src/server.js",
    "test": "jest --runInBand --verbose"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "gianantoniopini",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "got": "^11.8.2",
    "http-status-codes": "^2.2.0",
    "mongoose": "^7.1.0",
    "tough-cookie": "^4.1.2",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.0.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
