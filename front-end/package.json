    "axios-mock-adapter": "^1.21.5",
    "eslint": "^8.39.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "http-status-codes": "^2.2.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.33.0",
    "vitest-canvas-mock": "^0.3.2",
    "vue-tsc": "^1.6.5"
