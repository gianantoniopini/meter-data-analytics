{
  "name": "meter-data-analytics-front-end",
  "version": "1.0.0",
  "private": true,
  "description": "Vue app with TypeScript and Axios",
  "author": "gianantoniopini",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
